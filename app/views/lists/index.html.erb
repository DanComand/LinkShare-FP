<h1>Lists#index</h1>
<p>Find me in app/views/lists/index.html.erb</p>

<h1>Create a new list</h1>

<%= form_for List.new do |f| %>

	<div class="field">
		<%= f.label :name %><br>
		<%= f.text_field :name %>
	</div>

<div class="actions">
	<%= f.submit %>
</div>


<% end %>

<% @list.each do |list| %>

<p><%= link_to list.name, "/lists/#{list.id}" %></p>

<% end %>